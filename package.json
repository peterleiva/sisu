{
  "name": "sisu",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node .",
    "build": "npm run clean && npm run tsc",
    "build:watch": "npm run tsc -- --watch",
    "tsc": "tsc -p tsconfig.build.json",
    "dev": "concurrently \"npm:build:watch\" \"nodemon dist/main.js\"",
    "lint": "eslint .",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "prettier": "prettier --ignore-path .gitignore \".\"",
    "test": "jest",
    "prepare": "husky install",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pherval/sisu.git"
  },
  "keywords": [
    "sisu-enem",
    "enem",
    "simulador"
  ],
  "author": "pleiva <npmjs@peter.dev.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pherval/sisu/issues"
  },
  "homepage": "https://github.com/pherval/sisu#readme",
  "dependencies": {
    "cli-progress": "^3.10.0",
    "csv-parse": "^5.0.4",
    "dotenv": "^16.0.0",
    "mongoose": "^6.4.6",
    "node-environment": "^0.5.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/cli-progress": "^3.9.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "mongodb-memory-server": "^8.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.--write": "prettier --write --ignore-unknown"
  }
}
